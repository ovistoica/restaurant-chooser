{
  "parserOptions": {
    "parser": "babel-eslint",
    "sourceType": "module",
    "ecmaVersion": 2019,
    "ecmaFeatures": {
            "jsx": true,
            "modules": true
        }
  },
  "extends": "airbnb",
  "env": {
    "jest": true
  },
  "rules": {
    "no-use-before-define": "off",
    "react/jsx-filename-extension": "off",
    "react/prop-types": "off",
    "comma-dangle": "off"
  },
  "globals": {
    "fetch": false
  }
}